@model Lab5.Models.Schedule

<h1>Schedule Details</h1>

<div>
    <dl class="row">
        <dt class="col-sm-2">Group</dt>
        <dd class="col-sm-10">@Model.Group.Name</dd>

        <dt class="col-sm-2">Instructor</dt>
        <dd class="col-sm-10">@Model.Instructor.Name</dd>

        <dt class="col-sm-2">Classroom</dt>
        <dd class="col-sm-10">@Model.Classroom.Number</dd>

        <dt class="col-sm-2">Time</dt>
        <dd class="col-sm-10">@Model.ClassPeriod.Time</dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</div>